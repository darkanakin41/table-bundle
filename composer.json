{
    "name": "plejeune/table-bundle",
    "type": "symfony-bundle",
    "description": "Pierre LEJEUNE TableBundle",
    "keywords": [
        "Table Bundle"
    ],
    "homepage": "https://gitlab.com/pierrelejeune/tablebundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre LEJEUNE",
            "email": "lejeune.pierre.41@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "knplabs/knp-paginator-bundle": "dev-master",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/orm-pack": "*",
        "symfony/serializer-pack": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.3",
        "symfony/console": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0"
        }
    },
    "autoload": {
        "psr-4": {
            "PLejeune\\TableBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PLejeune\\TableBundle\\Tests\\": "/Tests/"
        }
    }
}
